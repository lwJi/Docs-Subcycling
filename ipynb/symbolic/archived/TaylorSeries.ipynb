   "source": [
    "function Series(y, t, t0, ord)\n",
    "    Dt = Differential(t)\n",
    "    return y(t0) + sum((Dt^i)(y(t0))/factorial(i)*(t-t0) for i in 1:ord)\n",
    "end"
   ]
  },
  {
